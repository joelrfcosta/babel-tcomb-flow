{
  "name": "babel-tcomb-flow",
  "version": "1.0.0",
  "description": "Node app sample with babel, tcomb and flow",
  "author": "Joel Costa <joelrfcosta@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ],
    "passPerPreset": true,
    "plugins": [
      "syntax-flow",
      "tcomb",
      "transform-flow-strip-types",
      "transform-react-jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-tcomb": "^0.3.11",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "flow-bin": "^0.30.0",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "tcomb": "^3.2.12",
    "tcomb-form": "^0.9.10",
    "tcomb-validation": "^3.2.1"
  }
}
